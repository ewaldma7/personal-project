generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  user_id       Int    @id @default(autoincrement())
  name          String @db.VarChar(255)
  email         String @unique @db.VarChar(100)
  password_hash String @db.VarChar(255)
  role          Int    @default(0)
}
